diff --git a/dlls/mfc/afxstate.c b/dlls/mfc/afxstate.c
index 43f0754fe5e..9c74b60a109 100644
--- a/dlls/mfc/afxstate.c
+++ b/dlls/mfc/afxstate.c
@@ -17,6 +17,7 @@
  */
 
 #include "wine/debug.h"
+#include "wine/asm.h"
 
 #include "winbase.h"
 
@@ -43,4 +44,20 @@ struct c_runtime_class
 void WINAPI AfxClassInit(struct c_runtime_class *runtime)
 {
     FIXME("%p\n", runtime);
+}
+
+/* CWinAppEx */
+struct c_winappex
+{
+    BOOL smartupdate;
+};
+
+/*
+ * ??0CWinAppEx@@QAE@H@Z
+ * ??0CWinAppEx@@QEAA@H@Z
+ */
+DEFINE_THISCALL_WRAPPER(cwinappex_ctor, 8)
+void __thiscall cwinappex_ctor(struct c_winappex *winapp, int smart_update)
+{
+    FIXME("%p, %d\n", winapp, smart_update);
 }
\ No newline at end of file
